<%= turbo_frame_tag 'results_table' do %>
  <table class="table">
    <thead>
    <tr>
      <th scope="col">Course</th>
      <th scope="col">Student</th>
      <th scope="col">Grade</th>
    </tr>
    </thead>
    <tbody>
    <% results.each do |result| %>
      <tr>
        <th scope="row"><%= result.course.name %></th>
        <td><%= result.student.full_name %></td>
        <td><%= result.grade %></td>
      </tr>
    <% end %>
    </tbody>
  </table>
<% end %>
